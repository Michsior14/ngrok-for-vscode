{
  "name": "ngrok-for-vscode",
  "version": "1.2.0",
  "displayName": "ngrok for VSCode",
  "description": "A VSCode extension to control ngrok from the command palette.",
  "keywords": [
    "ngrok",
    "localtunnel",
    "tunnel",
    "http",
    "webhooks"
  ],
  "author": {
    "name": "Phil Nash",
    "email": "philnash@gmail.com",
    "url": "https://philna.sh/"
  },
  "publisher": "philnash",
  "homepage": "https://github.com/philnash/ngrok-for-vscode",
  "bugs": {
    "url": "https://github.com/philnash/ngrok-for-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philnash/ngrok-for-vscode.git"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ngrok-for-vscode.start",
    "onCommand:ngrok-for-vscode.stop",
    "onCommand:ngrok-for-vscode.dashboard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ngrok-for-vscode.start",
        "title": "start",
        "category": "ngrok"
      },
      {
        "command": "ngrok-for-vscode.stop",
        "title": "stop",
        "category": "ngrok"
      },
      {
        "command": "ngrok-for-vscode.dashboard",
        "title": "dashboard",
        "category": "ngrok"
      }
    ],
    "configuration": {
      "title": "ngrok for VSCode",
      "properties": {
        "ngrokForVSCode.configPath": {
          "type": "string",
          "default": "",
          "description": "The location of your ngrok config file."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/mkdirp": "^1.0.0",
    "@types/yaml": "^1.2.0",
    "mkdirp": "^1.0.4",
    "ngrok": "^3.2.7",
    "yaml": "^1.8.3"
  },
  "license": "MIT"
}
