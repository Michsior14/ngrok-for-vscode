{
  "name": "ngrok-for-vscode",
  "displayName": "ngrok for VSCode",
  "description": "A VSCode extension to control ngrok",
  "keywords": [
    "ngrok",
    "localtunnel",
    "tunnel"
  ],
  "author": {
    "name": "Phil Nash",
    "email": "philnash@gmail.com",
    "url": "https://philna.sh/"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ngrok-for-vscode.start",
    "onCommand:ngrok-for-vscode.stop",
    "onCommand:ngrok-for-vscode.dashboard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ngrok-for-vscode.start",
        "title": "start",
        "category": "ngrok"
      },
      {
        "command": "ngrok-for-vscode.stop",
        "title": "stop",
        "category": "ngrok"
      },
      {
        "command": "ngrok-for-vscode.dashboard",
        "title": "dashboard",
        "category": "ngrok"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "ngrok": "^3.2.7"
  },
  "license": "MIT"
}
